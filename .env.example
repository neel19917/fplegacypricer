# Airtable Configuration
# Get your API key from: https://airtable.com/create/tokens
VITE_AIRTABLE_API_KEY=your_airtable_api_key_here

# Airtable Base ID (found in Airtable API documentation for your base)
VITE_AIRTABLE_BASE_ID=your_base_id_here

# Airtable Table ID or Name (the table containing pricing data)
VITE_AIRTABLE_TABLE_ID=your_table_id_or_name_here

# Polling interval in minutes (optional, default is 5 minutes)
VITE_AIRTABLE_POLL_INTERVAL=5

# Enable/disable Airtable (optional, default is true if API key exists)
# Set to "false" to disable Airtable and use JSON only
VITE_USE_AIRTABLE=true

# Master Password (existing)
VITE_MASTER_PASSWORD=default-password-set-in-env
